{
  "include": [
      "./src/*"
  ],
  "exclude": [
      "src/**/*.test.ts",
      "src/**/*.spec.ts"
  ],
  "compilerOptions": {
      "strict": true,
      "esModuleInterop": true,
      "allowUnreachableCode": false,
      "forceConsistentCasingInFileNames": true,
      "lib": [
          "dom",
          "dom.iterable",
          "esnext"
      ],
      "jsx": "react"
  }
}
